<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acer.osadd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AfficheOS" />
        <activity android:name=".ADD_OS" />
        <activity
            android:name=".SQLiteActivity"
            android:label="@string/title_activity_sqlite"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FileActivity"
            android:label="@string/title_activity_file"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".JSONActivity"
            android:label="@string/title_activity_json"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Affiche_json"></activity>
    </application>

</manifest>